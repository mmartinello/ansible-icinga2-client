object Endpoint "{{ icinga2_master_host }}" {
}

object Zone "{{ icinga2_master_host }}" {
	endpoints = [ "{{ icinga2_master_host }}" ]
}

object Endpoint "{{ inventory_hostname }}" {
}

object Zone "{{ inventory_hostname }}" {
	endpoints = [ "{{ inventory_hostname }}" ]
	parent = "{{ icinga2_master_host }}"
}

object Zone "global-templates" {
	global = true
}

object Zone "director-global" {
	global = true
}

{% if ansible_os_family == "Windows" %}
object Zone "windows-templates" {
        global = true
}
{% endif %}
